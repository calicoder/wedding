                    %label.label_title
                      %strong Last name:
                    %input.inputtext.input_middle.required{:name => "l_name", :type => "text", :value => ""}/
                  .clear
                  .row.field_textarea
                    %label.label_title
                      %strong Your message:
                    %textarea.textarea.textarea_middle.required{:cols => "30", :name => "message", :rows => "10"}
                  .row.rowSubmit
                    %input.btn-submit{:type => "submit", :value => "ADD COMMENT"}/
